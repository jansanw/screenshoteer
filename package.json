{
  "name": "screenshoteer",
  "version": "1.1.1",
  "description": "Make screenshots and device emulations form your terminal",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "NODE_PATH=/usr/bin/node PORT=8060 /usr/bin/node ./server.js",
    "proxy": "NODE_PATH=/usr/bin/node PORT=8060 PROXY=127.0.0.1:1080 /usr/bin/node ./server.js",
    "shot": "node ./index.js --proxy=http://127.0.0.1:1080 --url https://www.google.com --file google.png"
  },
  "bin": {
    "screenshoteer": "index.js"
  },
  "dependencies": {
    "commander": "^6.2.0",
    "puppeteer": "^5.4.0"
  },
  "keywords": [
    "screenshots",
    "emulations",
    "mobile"
  ],
  "author": "Vladimir Carrer <vladocar@gmail.com> (http://www.vcarrer.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vladocar/screenshoteer.git"
  },
  "bugs": {
    "url": "https://github.com/vladocar/screenshoteer/issues"
  },
  "homepage": "https://github.com/vladocar/screenshoteer#readme"
}
